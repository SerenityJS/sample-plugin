    "@serenityjs/entity": "*",
    "@serenityjs/item": "*",
    "@serenityjs/logger": "*",
    "@serenityjs/nbt": "*",
    "@serenityjs/network": "*",
    "@serenityjs/plugins": "*",
    "@serenityjs/protocol": "*",
    "@serenityjs/raknet": "*",
    "@serenityjs/serenity": "*",
    "@serenityjs/server-ui": "*",
    "@serenityjs/typescript-config": "*",
    "@serenityjs/world": "*"
  },
  "devDependencies": {
    "tsup": "^8.2.4"
  }
